<h2 class="text-center">Log in</h2>
<div class="row-fluid">
  <div class="span4 well">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
 <div class="control-group">
    <%= f.label :email,:class => "control-label"  %><br />
    <div class="controls">
    <%= f.email_field :email, autofocus: true, autocomplete: "email",:class => "input-group input-group-lg"%>
  </div>
 <br>
  <div class="control-group">
    <%= f.label :password, :class => "control-label"%><br />
    <%= f.password_field :password, autocomplete: "current-password",:class => "input-group input-group-lg"%>
  </div>
<br>
  <% if devise_mapping.rememberable? %>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end %>
<br>
  <div class="form-actions">
    <%= f.submit "Sign In", class: 'btn btn-success' %>
     <%= render "devise/shared/links" %>
  </div>
 
<% end %>

